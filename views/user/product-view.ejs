            selectedSize = size;
            selectedProductStock = quantity;
            console.log(size, quantity)
            if(quantity == 0){
                document.getElementById("stockAvailability").textContent = 'Out of Stock'
                document.getElementById("stockAvailability").style.color = '#ff4d4d'
            } else {
                if(quantity < 10) {
                    document.getElementById("stockAvailability").textContent = `Only ${quantity} items left in stock`
                    document.getElementById("stockAvailability").style.color = '#ff4d4d'
                } else {
                    document.getElementById("stockAvailability").textContent = 'Stock Available'
                    document.getElementById("stockAvailability").style.color = '#009933'
                }
            }
        }

        async function addToCart(productId){
            if(selectedSize == undefined){
                // event.preventDefault()
                document.getElementById('sizeSelectionError').textContent = 'Select a size to add to cart'
            } else if(selectedProductStock == 0) {
                // event.preventDefault()
                document.getElementById('sizeSelectionError').textContent = 'Product is out of stock'
            } else {
                let response = await fetch('/addtocart', {
                    method: "POST",
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({productId, selectedSize})
                })
                if(!response.ok){
                    console.log(`error ---${response.statusText}`)
                } else {
                    window.location.href = "/cart"
                }
                const data = await response.json()
                console.log(`Data is ${data}`)
            }
        }

        async function addToWishlist(productId) {
            console.log('Selected product Id - ', productId)
            await fetch('/addtowishlist', {
                method: "PUT",
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({ productId })
            }).then(response => {
                if (response.ok) {
                    return response.json();
                } else {
                    throw new Error('Failed to add to wishlist');
                }
            }).then(data => {
                console.log(data.message);
                if(data.message == 'Already added to wishlist'){
                    Swal.fire({
                    icon: 'error',
                    title: 'Product already added to wishlist',
                    // text: 'The product has been successfully added to your wishlist!'
                });
                } else {
                    Swal.fire({
                        icon: 'success',
                        title: 'Success',
                        text: 'The product has been successfully added to your wishlist!'
                    });
                }
            }).catch(error => {
                console.error(error);
                Swal.fire({
                    icon: 'error',
                    title: 'Login Required',
                    text: 'Login to add the product to wishlist',
                    showCancelButton: true,
                    confirmButtonText: 'Login',
                    cancelButtonText: 'Cancel'
                }).then((result) => {
                    if (result.isConfirmed) {
                        window.location.href = '/login'; // Change '/login' to your actual login page URL
                    }
                });
            });
        }


    </script>
